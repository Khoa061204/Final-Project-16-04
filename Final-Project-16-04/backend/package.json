{
  "name": "cloudsync-backend",
  "version": "1.0.0",
  "description": "CloudSync Backend API Server - Enterprise File Management & Collaboration Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Backend build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d src/config/database.js",
    "db:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.js",
    "db:seed": "node src/utils/seedDatabase.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "file-management",
    "collaboration",
    "team-management",
    "project-management",
    "cloud-storage",
    "api",
    "express",
    "mysql",
    "typeorm"
  ],
  "author": "CloudSync Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.858.0",
    "@aws-sdk/s3-request-presigner": "^3.858.0",
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typeorm-cli": "^1.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/cloudsync.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/cloudsync/issues"
  },
  "homepage": "https://cloudsync.com"
}
